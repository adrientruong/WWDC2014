OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/CPAnimationSequence" -isystem "${PODS_ROOT}/Headers/ColorArt" -isystem "${PODS_ROOT}/Headers/LEColorPicker" -isystem "${PODS_ROOT}/Headers/Shimmer"
